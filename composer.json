{
    "name": "rhymedigital/contao_rhyme_formdata",
    "description": "A Contao CMS extension that dynamically stores form data from the form generator (replacement for EFG) with Excel/CSV export",
    "keywords": [
    	"efg", 
    	"contao",
    	"formdata"
    ],
    "type": "contao-module",
    "license": "LGPL-3.0+",
    "authors": [
        {
            "name": "Rhyme Digital",
            "homepage": "http://rhyme.digital"
        }
    ],
    "require": {
        "php": ">=5.3.2",
        "contao/core": ">=3.3.0,<4.0.0-dev",
        "contao-community-alliance/composer-installer": "*",
        "terminal42/contao-namespace-class-loader":"~1.0",
        "phpoffice/phpexcel": "dev-master"
    },
    "autoload": {
        "psr-0": {
            "Rhyme\\": [
                "library/"
            ]
        }
    },
    "extra": {
        "contao": {
            "sources": {
                "": "system/modules/rhyme_formdata"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "preferred-install": "dist"
    }
}